{
  //https://developer.chrome.com/docs/extensions/mv3/intro/mv3-migration/
  "manifest_version": 2,

  "converted_from_user_script": true,
  "name": "Download WebSite Media",
  "description": "Adds a button that lets you download media files from platforms: RedBull, ServusTv, MySpass, Vimeo, YouTube, Dailymotion, MTV, TED, Airmeet",
  "version": "0.5.11",
  "icons": { "64": "./DLWSMEDIA.png" },
  "content_scripts": [
    {
      "js": [ "./DLWSMEDIA.user.js" ],
      "matches": [
        "https://*.myspass.de/*",
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  //"permissions": [ "tabs", "*://*/*.m3u8*", "webRequest", "webRequestBlocking", "storage" ],
  "web_accessible_resources": [ "*.js" ],

  "permissions": [ "tabs", "<all_urls>", "webRequest", "webRequestBlocking", "storage" ],
  "background": {
    "scripts": [ "event.js" ]
  },
  "browser_action": {
    "default_icon": "./DLWSMEDIA.png",
    "default_popup": "popup.html",
    "default_title": "Disable"
  },
  "omnibox": {
    "keyword": "m3u8"
  }

  //"content_security_policy": "script-src 'self' blob:; object-src 'self'"
}
